<launch>
	<arg name="mimic" default="false"/>
	<include file="$(find file_server)/launch/ros_sharp_communication.launch">
		<arg name="port" value="9090" />
	</include>
	
	<param name="robot/name" value="Avatar" />
	<param name="robot_description" command="$(find xacro)/xacro $(find avatar_robot_description)/franka/dual_panda/dual_panda_example.urdf.xacro mimic:=$(arg mimic)"/>
	<!-- <param name="robot_description" command="$(find xacro)/xacro $(find avatar_gripper_description)/urdf/avatar_gripper_3f_example.xacro mimic:=$(arg mimic) pref:='left'"/> -->
</launch>
